			<FemaleText/>
		</Entry>
		<Entry>
			<ID>2735</ID>
			<DefaultText>免疫：火焰</DefaultText>
			<FemaleText/>
		</Entry>
		<Entry>
			<ID>2736</ID>
			<DefaultText>交戰打擊</DefaultText>
			<FemaleText/>
